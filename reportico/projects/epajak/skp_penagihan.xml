<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Laporan SKP Penagihan</ReportTitle><ReportDescription>Rentang tanggal berdasarkan Tanggal Entri</ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>.DEFAULT</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize></pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>.DEFAULT</gridDisplay><gridSortable>.DEFAULT</gridSortable><gridSearchable>.DEFAULT</gridSearchable><gridPageable>.DEFAULT</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>select skp.nomor_skprd as Nomor_SKPRD, skp.tanggalentri as Tanggal_Entri, sptpd.keteranganpajak as Keterangan_Pajak, sptpd.namawp as Nama_Wajib_Pajak, skp.penyetor as Penyetor, sptpd.jumlahpajak as Ketetapan, tandabayar.jumlahpajak as Terbayar, skp.tglbayar as Tanggal_Bayar, sptpd.jenispajak, skp.masa1 as Masa_1, skp.masa2 as Masa_2, skp.lunas as Lunas
from skp
left join tandabayar on tandabayar.nomor_skprd=skp.nomor_skprd
left join sptpd on sptpd.noid=skp.nomor_sptpd
where 
skp.tanggal_terbit between {daterange,RANGE1} and {daterange,RANGE2} and skp.keterangan=0 and skp.aktif=1
order by sptpd.jenispajak

